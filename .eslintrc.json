{
  "env": {
    "browser": true,
    "es6": true,
    "es2020": true
  },
  "parserOptions": {
    "ecmaVersion": 11
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb",
    "prettier"
  ],
  "plugins": ["react-hooks", "prettier", "react"],
  "rules": {
    "react/jsx-wrap-multilines": [
      "error",
      { "declaration": false, "assignment": false }
    ],
    "prettier/prettier": "warn",
    "no-unused-vars": "warn",
    "no-console": "warn"
  }
}
